    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^10.4.0",
    "gh-pages": "^6.3.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
