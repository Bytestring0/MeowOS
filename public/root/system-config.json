{
  "name": "MeowOS System Configuration",
  "version": "1.0.0",
  "features": {
    "animations": {
      "enabled": true,
      "presets": ["none", "default", "smooth", "fast", "bouncy"],
      "customizable": true
    },
    "fileSystem": {
      "realFS": true,
      "operations": ["create", "delete", "browse", "preview"]
    },
    "persistence": {
      "storage": "IndexedDB",
      "settings": ["theme", "wallpaper", "appStates"]
    },
    "desktop": {
      "draggableWindows": true,
      "taskbar": true,
      "iconSystem": "SVG"
    }
  },
  "lastUpdate": "2024-12-19",
  "status": "stable"
}
